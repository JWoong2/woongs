<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="income">
	<select id="findIncomeById" parameterType="map" resultType="map">
		SELECT * 
		FROM T_PAID_HISTORY
		WHERE USER_ID = #{USER_ID} 
	</select>
	
	<select id="findAccountInfoByUserId" parameterType="map" resultType="map">
		SELECT *
		FROM accountinfo
		WHERE USER_ID = #{USER_ID}	
	</select>

</mapper>
