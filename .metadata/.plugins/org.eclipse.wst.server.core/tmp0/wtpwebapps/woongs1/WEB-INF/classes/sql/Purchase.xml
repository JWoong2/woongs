<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="purchase">
	<select id="pusrchaseClientId" parameterType="map" resultType="map">
		SELECT * 
		FROM workorder
		WHERE client_id = #{client_id} 
	</select>
</mapper>
