package com.woongjin.woongs.model;

import java.util.List;
import java.util.Map;

import org.mybatis.spring.support.SqlSessionDaoSupport;

public class PurchaseDao extends SqlSessionDaoSupport {
	public List<Map<String, Object>> pusrchaseClientId(Map<String, Object> info) {
		// TODO Auto-generated method stub
		return getSqlSession().selectList("purchase.pusrchaseClientId", info);
	}
}
