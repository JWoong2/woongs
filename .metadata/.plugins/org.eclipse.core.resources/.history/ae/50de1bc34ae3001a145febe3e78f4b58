<mapper namespace="income3">
	<select id="findIncomeById3" parameterType="String" resultType="com.woongjin.woongs.model.T_Paid_HistoryDto">
		SELECT * 
		FROM T_PAID_HISTORY
		WHERE USER_ID = #{USER_ID} 
	</select>

	<select id="money1" parameterType="String" resultType="com.woongjin.woongs.model.BalanceDto">
		SELECT *
		FROM balance
		WHERE USER_ID = #{USER_ID} 
	</select>
</mapper>