package com.woongjin.woongs.model;

import java.util.List;
import java.util.Map;

import org.mybatis.spring.support.SqlSessionDaoSupport;

public class Income3Dao extends SqlSessionDaoSupport {
	public Map<String, Object> findIncomeById3(String user_id) {
		// TODO Auto-generated method stub
		return getSqlSession().selectOne("income3.findIncomeById3", user_id);
	}

	public Map<String, Object> findMoneyById(String user_id) {
		// TODO Auto-generated method stub
		return getSqlSession().selectOne("income3.money1", user_id);
	}
}
