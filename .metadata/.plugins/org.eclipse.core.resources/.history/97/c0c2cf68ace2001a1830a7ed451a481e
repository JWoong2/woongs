<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
<script>
var income = {
			init : function() {
				income.find_income_by_user_id();

			},
			find_income_by_user_id : function() {

				$.ajax({
					type : "GET",
					url : "findIncomeByUserId",
					dataType : 'json'
				}).done(function(datas) { 
					var sumPrice = 0;
					var finshYIncome = 0;
					var finshNIncome = 0;

					datas.forEach(function(item, index, data) {
						if (item.FINSH_YN == "Y") {
							finshYIncome += item.PRICE;
						} else {
							finshNIncome += item.PRICE;
						}
						sumPrice += item.PRICE;
	
					$("#totalIncome").html(sumPrice + "¿ø");
					$("#finshYIncome").html(finshYIncome + "¿ø");
					$("#finshNIncome").html(finshNIncome + "¿ø");

			</script>
</body>
</html>