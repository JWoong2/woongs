$(document).ready(function(){
	$('.logout').click(function(){
		alert('로그아웃 되었습니다')
	})
	
	$('.wish').click(function(){
	alert('1');
       if ($(this).hasClass("on")) {
	       $(this).removeClass("on");
           $(this).addClass("off");
           $(this).children('img').attr('src', '/woongs/resources/Images/emptyheart.png'); 
       }else{
           $(this).removeClass("off");
           $(this).addClass("on");
           $(this).children('img').attr('src', '/woongs/resources/Images/heart.png'); 
       }
    });
})